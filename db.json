DROP DATABASE if exists products;
CREATE DATABASE products;
USE products;
CREATE TABLE items (
    item_id INT AUTO_INCREMENT,
    name varchar(20),
    category varchar(20),
    price decimal (3, 2),
    model varchar(20),
    size varchar(20),
    primary key(item_id)
);
